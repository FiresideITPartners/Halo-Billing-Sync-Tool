{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "organizations": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "name": {
                        "type": "string"
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "account_id": {
                        "type": "integer"
                    },
                    "key": {
                        "type": "string"
                    },
                    "notify_emails": {
                        "type": "array",
                        "items": {
                            "type": "string",
                            "format": "email"
                        }
                    },
                    "microsoft_365_tenant_id": {
                        "type": ["string", "null"]
                    },
                    "incident_reports_count": {
                        "type": "integer"
                    },
                    "agents_count": {
                        "type": "integer"
                    },
                    "microsoft_365_users_count": {
                        "type": "integer"
                    }
                },
                "required": [
                    "id",
                    "name",
                    "created_at",
                    "updated_at",
                    "account_id",
                    "key",
                    "notify_emails",
                    "incident_reports_count",
                    "agents_count",
                    "microsoft_365_users_count"
                ]
            }
        }
    },
    "required": ["organizations"]
}